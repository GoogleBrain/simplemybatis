<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yufu.main.mapper.StudentMapper">

    <select id="selectById" resultType="cn.yufu.main.domain.Student">
        select id,name  from student where id = #{param1} and name=#{param2}
    </select>
</mapper>